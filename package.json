{
  "name": "prettier-ui",
  "version": "1.0.4",
  "description": "UI",
  "main": "dist/prettier.js",
  "files": [
    "dist",
    "src"
  ],
  "private": false,
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --compress  --config build/webpack.dev.config.js",
    "create:md": "node build/.bin/createMd.js",
    "dist:prettier": "webpack  --config build/webpack.dist.prod.config.js",
    "dist:style": "gulp --gulpfile build/build-style.js",
    "dist:dev": "webpack --config build/webpack.dist.dev.config.js",
    "dist:docs": "webpack --config build/webpack.dist.docs.config.js",
    "dist:locale": "webpack --config build/webpack.dist.locale.config.js",
    "dist": "npm run dist:prettier && npm run dist:style && npm run dist:locale",
    "lint": "eslint --fix --ext .js,.vue src",
    "unit": "cross-env BABEL_ENV=test karma start test/karma.conf.js --single-run",
    "test": "npm run unit",
    "release": "sh build/release.sh",
    "test:coverage": "find test/unit/coverage/lcov-report -name 'index.html' | sed -n 1,1p  | xargs -I {} open {}",
    "deploy": "npm run dist:docs && gh-pages -d examples/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonliao001/prettier.git"
  },
  "author": "bxliao",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jasonliao001/prettier/issues"
  },
  "homepage": "https://github.com/jasonliao001/prettier#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "gulp": "^4.0.1",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "highlight.js": "^9.15.6",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^4.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.5",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "markdown-it": "^8.4.2",
    "markdown-it-container": "^2.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "mocha": "^6.1.4",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-load-config": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.7.0",
    "vue-markdown-loader": "^2.4.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "deepmerge": "^3.2.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6"
  },
  "browserslist": "> 0.25%, not dead"
}
